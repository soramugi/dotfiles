#!/bin/bash

for i in {0..255} ; do
  printf "\x1b[38;5;${i}mcolour${i}  "
done
echo ''

echo -e '
状態番号;色番号

状態番号
\033[0m 0        デフォルト状態\033[00m
\033[1m 1        強調          \033[00m
\033[4m 4        下線          \033[00m
\033[5m 5        点滅          \033[00m

色番号
\033[0;30m 30 文字表示色:黒               \033[00m \033[0;40m 40 背景色:黒               \033[00m
\033[0;31m 31 文字表示色:赤               \033[00m \033[0;41m 41 背景色:赤               \033[00m
\033[0;32m 32 文字表示色:緑               \033[00m \033[0;42m 42 背景色:緑               \033[00m
\033[0;33m 33 文字表示色:黄(または茶)     \033[00m \033[0;43m 43 背景色:黄(または茶)     \033[00m
\033[0;34m 34 文字表示色:青               \033[00m \033[0;44m 44 背景色:青               \033[00m
\033[0;35m 35 文字表示色:紫               \033[00m \033[0;45m 45 背景色:紫               \033[00m
\033[0;36m 36 文字表示色:シアン           \033[00m \033[0;46m 46 背景色:シアン           \033[00m
\033[0;37m 37 文字表示色:白(またはグレー) \033[00m \033[0;47m 47 背景色:白(またはグレー) \033[00m
'
